<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="TryTry.App">
	<Application.Resources>
		<!-- Application resource dictionary -->
		<ResourceDictionary>
			<Style x:Key="labelstyle1_app" TargetType="Label">
				<Setter Property="TextColor" Value="Yellow"/>
			</Style>
			<Style x:Key="labelstyle2_app" TargetType="Label">
				<Setter Property="TextColor" Value="Pink"/>
			</Style>
		</ResourceDictionary>
	</Application.Resources>
</Application>
